{
    "version": 2,
    "configurePresets": [
        {
            "name": "Ubuntu_Clang9_V8",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/.build/${presetName}",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "/usr/bin/clang-9",
                "CMAKE_CXX_COMPILER": "/usr/bin/clang++-9",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/.install/${presetName}",
                "NAPI_JAVASCRIPT_ENGINE": "V8"
            }
        },
        {
            "name": "Ubuntu_GCC9_JSC",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/.build/${presetName}",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "/usr/bin/gcc-9",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++-9",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/.install/${presetName}",
                "NAPI_JAVASCRIPT_ENGINE": "JavaScriptCore",
                "JSCORE_LIBRARY": "/usr/lib/x86_64-linux-gnu/libjavascriptcoregtk-4.0.so"
            }
        },
        {
            "name": "Ubuntu_GCC9_V8",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/.build/${presetName}",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "/usr/bin/gcc-9",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++-9",
